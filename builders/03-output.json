{
  "variables": {
    "build_version": "",
    "box_name": "",
    "box_output_path": ""
  },
  "builders": [
    {
      "type": "virtualbox-ovf",
      "source_path": "ovf_outputs/applications/02_applications.ovf",
      "vm_name": "03_vagrant",
      "communicator": "none",
      "virtualbox_version_file": "",
      "headless": true,
      "boot_wait": "0s",
      "guest_additions_mode": "disable"
    }
  ],
  "post-processors": [
    {
      "compression_level": "9",
      "output": "{{user `box_output_path`}}",
      "type": "vagrant"
    }
  ]
}